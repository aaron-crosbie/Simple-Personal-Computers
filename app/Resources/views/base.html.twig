<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>SPC - {% block title %}{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <!-- Disable tap highlight on IE -->
    <meta name="msapplication-tap-highlight" content="no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <!-- Add to home-screen for Chrome on Android -->
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" sizes="192x192" href="../../../web/images/icon.png">

    <!-- Add to home-screen for Safari on iOS -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <link rel="apple-touch-icon" href="../../../web/images/icon.png">

    <!-- Tile icon for Win8 (144x144 + tile color) -->
    <meta name="msapplication-TileImage" content="../public/images/icon.png">
    <meta name="msapplication-TileColor" content="#2F3BA2">

    <!-- Color the status bar on mobile devices -->
    <meta name="theme-color" content="#2F3BA2">
    {% block stylesheets %}

    {% endblock %}

        <!-- Personal css style sheets -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/nav.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/footer.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/modal.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/forms.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/reviews.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/script.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/build.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/styles.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/vertical_carousal.min.css') }}">




    <!-- jQuery JS implementation -->


    <!-- Google selected fonts -->
    <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    {% block javascripts %}

    {% endblock %}
</head>

<nav>
    <ul>
        <li>
            <a
                {% if app.request.attributes.get('_route') == 'homepage'  %}

                id="current"

                {% endif %}

                href="/"><span>Home</span>
            </a>
        </li>

        <li>
            <a
                {% if app.request.attributes.get('_route') == 'buildpage'  %}

                    id="current"

                {% endif %}

                href="/build"><span>Build SPC</span>
            </a>
        </li>

        <li>
            <a
                {% if app.request.attributes.get('_route') == 'blogpage'  %}

                    id="current"

                {% endif %}

                href="/blog"><span >Blog</span>
            </a>
        </li>

        <li>
            <a
                {% if app.request.attributes.get('_route') == 'surveypage'  %}

                    id="current"

                {% endif %}

                href="/survey"><span>Survey</span>
            </a>
        </li>

        <li>
            <a
                {% if app.request.attributes.get('_route') == 'documentpage'  %}

                    id="current"

                {% endif %}

                href=/document"><span>Documentation</span>
            </a>
        </li>


        <li>
            {% if app.session.get('username') == '' %}
                <button id="myBtn" >Login</button>
            {% else %}
                <div style="font-size: 18px">
                    hi, <strong>{{ app.session.get('username')}}</strong>
                </div>

                <a
                        {% if app.request.attributes.get('_route') == 'accountpage'  %}

                            id="current"

                        {% endif %}
                        href="/account" style="padding: 8px 5px 0px; margin: 0"><img src="/images/account.png" style="width: 23px"></a>

                <a href="/logout" style="padding: 8px 5px 0px; margin: 0"><img src="/images/logout.png" style="width: 23px"></a>
            {% endif %}
        </li>
    </ul>

</nav>



<script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.3/jspdf.debug.js"></script>
<script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-36251023-1']);
    _gaq.push(['_setDomainName', 'jqueryscript.net']);
    _gaq.push(['_trackPageview']);

    (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

</script>
<body>
{% block body %}{% endblock %}
</body>

</html>
