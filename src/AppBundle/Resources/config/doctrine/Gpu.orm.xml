<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Gpu" table="gpu">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="manufacturer" type="string" column="manufacturer" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="memoryCap" type="string" column="memoryCap" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="processor" type="string" column="processor" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cardBus" type="string" column="cardBus" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="formFactor" type="string" column="formFactor" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="watts" type="integer" column="watts" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="price" type="integer" column="price" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
